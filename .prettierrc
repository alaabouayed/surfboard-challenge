{
  "printWidth": 100,
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "importOrder": [
    "^database",
    "^types",
    "^@slack-bot",
    "^@llm-models",
    "^@modules",
    "^@lib",
    "^[./|~/]",
    "^src",
    "^/resources"
  ],
  "importOrderSeparation": true,
  "importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
